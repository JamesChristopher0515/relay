  "dependencies": {
    "@bbuild/deps": "../deps"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/node": "^18.14.0",
    "typescript": "4.9.5"
  },
  "main": "dist/cjs/index.js",
  "index": "dist/cjs/index.js",
  "module": "dist/index.js"
