{
  "name": "@bbuild/json",
  "version": "0.0.1",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json",
    "./_deps": {
      "import": "./dist/_deps.js",
      "require": "./dist/cjs/_deps.js",
      "types": "./dist/_deps.d.ts"
    },
    "./index": {
      "import": "./dist/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/index.d.ts"
    },
    "./parse": {
      "import": "./dist/parse.js",
      "require": "./dist/cjs/parse.js",
      "types": "./dist/parse.d.ts"
    }
  },
  "dependencies": {
    "comment-json": "4.2.3",
    "json5": "2.2.3",
    "@bbuild/deps": "../deps",
    "@bbuild/file-parse": "../file-parse"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "typescript": "4.9.5"
  },
  "main": "dist/cjs/index.js",
  "index": "dist/cjs/index.js",
  "module": "dist/index.js"
}
